{
    "name": "auto-tfs",
    "displayName": "Auto TFS",
    "description": "TF command utils for VS Code",
    "version": "1.1.0",
    "publisher": "nikhil-patil",
    "license": "MIT",
    "author": {
        "name": "Nikhil Patil",
        "url": "https://github.com/nik-base"
    },
    "repository": "https://github.com/nik-base/auto-tfs",
    "homepage": "https://github.com/nik-base/auto-tfs#readme",
    "bugs": {
        "url": "https://github.com/nik-base/auto-tfs/issues"
    },
    "readmeFilename": "README.md",
    "engines": {
        "vscode": "^1.58.2"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "keywords": [
        "tfs",
        "team",
        "foundation",
        "server",
        "tf",
        "tfvc",
        "vsts",
        "azure",
        "microsoft",
        "auto",
        "auto-tfs",
        "checkout"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "auto-tfs.checkout",
                "title": "Auto TFS: Checkout current file"
            },
            {
                "command": "auto-tfs.undo",
                "title": "Auto TFS: Undo changes for current file"
            },
            {
                "command": "auto-tfs.add",
                "title": "Auto TFS: Add current file to TFS"
            },
            {
                "command": "auto-tfs.delete",
                "title": "Auto TFS: Delete current file"
            },
            {
                "command": "auto-tfs.vsdiff",
                "title": "Auto TFS: Compare current file in Visual Studio"
            },
            {
                "command": "auto-tfs.codediff",
                "title": "Auto TFS: Compare current file Here"
            },
            {
                "command": "auto-tfs.exp-checkout",
                "title": "Checkout"
            },
            {
                "command": "auto-tfs.exp-undo",
                "title": "Undo"
            },
            {
                "command": "auto-tfs.exp-add",
                "title": "Add"
            },
            {
                "command": "auto-tfs.tile-checkout",
                "title": "Checkout"
            },
            {
                "command": "auto-tfs.tile-undo",
                "title": "Undo"
            },
            {
                "command": "auto-tfs.tile-add",
                "title": "Add"
            },
            {
                "command": "auto-tfs.ide-checkout",
                "title": "Checkout"
            },
            {
                "command": "auto-tfs.ide-undo",
                "title": "Undo"
            },
            {
                "command": "auto-tfs.ide-add",
                "title": "Add"
            },
            {
                "command": "auto-tfs.get",
                "title": "Auto TFS: Get Latest of current file"
            },
            {
                "command": "auto-tfs.exp-get",
                "title": "Get Latest"
            },
            {
                "command": "auto-tfs.tile-get",
                "title": "Get Latest"
            },
            {
                "command": "auto-tfs.ide-get",
                "title": "Get Latest"
            },
            {
                "command": "auto-tfs.exp-delete",
                "title": "Delete"
            },
            {
                "command": "auto-tfs.tile-delete",
                "title": "Delete"
            },
            {
                "command": "auto-tfs.ide-delete",
                "title": "Delete"
            },
            {
                "command": "auto-tfs.exp-vsdiff",
                "title": "In Visual Studio"
            },
            {
                "command": "auto-tfs.tile-vsdiff",
                "title": "In Visual Studio"
            },
            {
                "command": "auto-tfs.ide-vsdiff",
                "title": "In Visual Studio"
            },
            {
                "command": "auto-tfs.exp-codediff",
                "title": "Here"
            },
            {
                "command": "auto-tfs.tile-codediff",
                "title": "Here"
            },
            {
                "command": "auto-tfs.ide-codediff",
                "title": "Here"
            }
        ],
        "submenus": [
            {
                "id": "auto-tfs/exp/action",
                "label": "Auto TFS"
            },
            {
                "id": "auto-tfs/tile/action",
                "label": "Auto TFS"
            },
            {
                "id": "auto-tfs/ide/action",
                "label": "Auto TFS"
            },
            {
                "id": "auto-tfs/exp/action/compare",
                "when": "!explorerResourceIsFolder && !listMultiSelection",
                "label": "Compare"
            },
            {
                "id": "auto-tfs/tile/action/compare",
                "when": "!explorerResourceIsFolder && !listMultiSelection",
                "label": "Compare"
            },
            {
                "id": "auto-tfs/ide/action/compare",
                "when": "!explorerResourceIsFolder && !listMultiSelection",
                "label": "Compare"
            }
        ],
        "menus": {
            "auto-tfs/exp/action": [
                {
                    "command": "auto-tfs.exp-get",
                    "group": "1_autotfs@1"
                },
                {
                    "command": "auto-tfs.exp-checkout",
                    "group": "1_autotfs@2"
                },
                {
                    "command": "auto-tfs.exp-undo",
                    "group": "1_autotfs@3"
                },
                {
                    "command": "auto-tfs.exp-add",
                    "group": "1_autotfs@4"
                },
                {
                    "command": "auto-tfs.exp-delete",
                    "group": "1_autotfs@5"
                },
                {
                    "submenu": "auto-tfs/exp/action/compare",
                    "when": "!explorerResourceIsFolder && !listMultiSelection",
                    "group": "1_autotfs@6"
                }
            ],
            "auto-tfs/tile/action": [
                {
                    "command": "auto-tfs.tile-get",
                    "group": "1_autotfs@1"
                },
                {
                    "command": "auto-tfs.tile-checkout",
                    "group": "1_autotfs@2"
                },
                {
                    "command": "auto-tfs.tile-undo",
                    "group": "1_autotfs@3"
                },
                {
                    "command": "auto-tfs.tile-add",
                    "group": "1_autotfs@4"
                },
                {
                    "command": "auto-tfs.tile-delete",
                    "group": "1_autotfs@5"
                },
                {
                    "submenu": "auto-tfs/tile/action/compare",
                    "when": "!explorerResourceIsFolder && !listMultiSelection",
                    "group": "1_autotfs@6"
                }
            ],
            "auto-tfs/ide/action": [
                {
                    "command": "auto-tfs.ide-get",
                    "group": "1_autotfs@1"
                },
                {
                    "command": "auto-tfs.ide-checkout",
                    "group": "1_autotfs@2"
                },
                {
                    "command": "auto-tfs.ide-undo",
                    "group": "1_autotfs@3"
                },
                {
                    "command": "auto-tfs.ide-add",
                    "group": "1_autotfs@4"
                },
                {
                    "command": "auto-tfs.ide-delete",
                    "group": "1_autotfs@5"
                },
                {
                    "submenu": "auto-tfs/ide/action/compare",
                    "when": "!explorerResourceIsFolder && !listMultiSelection",
                    "group": "1_autotfs@6"
                }
            ],
            "auto-tfs/exp/action/compare": [
                {
                    "when": "!explorerResourceIsFolder && !listMultiSelection",
                    "command": "auto-tfs.exp-codediff",
                    "group": "1_AutoTFS@1"
                },
                {
                    "when": "!explorerResourceIsFolder && !listMultiSelection",
                    "command": "auto-tfs.exp-vsdiff",
                    "group": "1_AutoTFS@2"
                }
            ],
            "auto-tfs/tile/action/compare": [
                {
                    "when": "!explorerResourceIsFolder && !listMultiSelection",
                    "command": "auto-tfs.tile-codediff",
                    "group": "1_AutoTFS@1"
                },
                {
                    "when": "!explorerResourceIsFolder && !listMultiSelection",
                    "command": "auto-tfs.tile-vsdiff",
                    "group": "1_AutoTFS@2"
                }
            ],
            "auto-tfs/ide/action/compare": [
                {
                    "when": "!explorerResourceIsFolder && !listMultiSelection",
                    "command": "auto-tfs.ide-codediff",
                    "group": "1_AutoTFS@1"
                },
                {
                    "when": "!explorerResourceIsFolder && !listMultiSelection",
                    "command": "auto-tfs.ide-vsdiff",
                    "group": "1_AutoTFS@2"
                }
            ],
            "explorer/context": [
                {
                    "submenu": "auto-tfs/exp/action",
                    "group": "4_autotfs@0"
                }
            ],
            "editor/title/context": [
                {
                    "submenu": "auto-tfs/tile/action",
                    "group": "4_autotfs@0"
                }
            ],
            "editor/context": [
                {
                    "submenu": "auto-tfs/ide/action",
                    "group": "4_autotfs@0"
                }
            ]
        },
        "configuration": {
            "title": "Auto TFS",
            "properties": {
                "auto-tfs.tf.path": {
                    "type": "string",
                    "description": "Full path to TF.EXE or TF.CMD"
                },
                "auto-tfs.tfs.confirm": {
                    "type": "boolean",
                    "description": "Show confirmation message on any TFS operation (i.e. checkout, add, delete, undo, etc.)"
                },
                "auto-tfs.tfs.autoCheckout": {
                    "type": "string",
                    "default": "Never",
                    "enum": [
                        "Never",
                        "On Save",
                        "On Change"
                    ],
                    "enumDescriptions": [
                        "Don't checkout file(s)",
                        "Checkout file(s) on save",
                        "Checkout file(s) on change"
                    ],
                    "description": "Specifies when should file(s) checked out"
                },
                "auto-tfs.tfs.autoAdd": {
                    "type": "boolean",
                    "description": "Add file to source control on adding file(s)"
                },
                "auto-tfs.tfs.autoDelete": {
                    "type": "boolean",
                    "description": "Delete file from source control on deleting file(s)"
                },
                "auto-tfs.tfs.autoRename": {
                    "type": "boolean",
                    "description": "Rename file on source control on renaming file(s)"
                },
                "auto-tfs.tfs.debug": {
                    "type": "boolean",
                    "description": "Run the extension in debug mode. Commands output will be printed, if the setting is set to true"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "lint": "node ./node_modules/tslint/bin/tslint --project tsconfig.json src/**/*.ts -t verbose"
    },
    "devDependencies": {
        "@types/node": "^16.4.8",
        "@types/vscode": "^1.58.1",
        "codelyzer": "^6.0.2",
        "tslint": "^6.1.3",
        "typescript": "^4.3.5"
    }
}
